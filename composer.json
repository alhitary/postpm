	"type": "phpbb-extension",
	"description": "An extension for phpBB 3.1.x that allows administrators to limit who can compose new PMs.",
	"homepage": "http://www.acsyste.com",
	"version": "1.0.0",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "kinerity",
			"email": "kinerity@yahoo.com",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "PostPM",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.4"
		}
	}
}
